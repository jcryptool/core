<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name"
            icon="icons/view.gif"
            category="org.jcryptool.analysis"
            class="org.jcryptool.analysis.vigenere.views.VigenereBreakerView"
            id="org.jcryptool.analysis.vigenere.views.VigenereBreakerView">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.analysis">
      <analysis
            name="%analysis.name"
            viewId="org.jcryptool.analysis.vigenere.views.VigenereBreakerView">
      </analysis>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.analysis.vigenere.handler.HelpHandler"
            commandId="org.jcryptool.analysis.vigenere.helpCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.jcryptool.analysis.vigenere.views.VigenereBreakerView">
         <command
               commandId="org.jcryptool.analysis.freqanalysis.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.gif"
               style="push"
               tooltip="%help.command.tooltip">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.gif">
         <command
               commandId="org.jcryptool.analysis.freqanalysis.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/save_edit.gif"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
