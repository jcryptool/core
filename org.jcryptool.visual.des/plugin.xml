<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.jcryptool.visual"
            class="org.jcryptool.visual.des.view.DesView"
          	id="org.jcryptool.visual.des.view.DesView"
            name="%view.name.0"
            icon="icons/view.gif"> 
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.visuals">
      <Visual
            name="%view.name.0"
            viewId="org.jcryptool.visual.des.view.DesView">
      </Visual>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.visual.des.handlers.RestartHandler"
            commandId="org.jcryptool.visual.des.restartCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.visual.des.restartCommand"
            name="%restart.command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.jcryptool.visual.des.view.DesView">
         <command
               commandId="org.jcryptool.visual.des.restartCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/new_wiz.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.jcryptool.visual.des.view.DesView">
         <command
               commandId="org.jcryptool.visual.des.restartCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/new_wiz.gif"
               style="push">
         </command>
      </menuContribution>
   </extension>
</plugin>
